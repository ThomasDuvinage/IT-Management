<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="300.0" prefWidth="521.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <HBox prefHeight="294.0" prefWidth="390.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER_RIGHT" prefHeight="294.0" prefWidth="170.0" spacing="20.0">
               <children>
                  <Label text="Nom :">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></Label>
                  <Label text="Prénom :" />
                  <Label text="Nom d'utilisateur :" />
                  <Label text="Mot de passe :" />
                  <Label text="Confirmez le mot de passe :" />
                  <Label text="Département :" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="294.0" prefWidth="235.0" spacing="12.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
               <children>
                  <TextField fx:id="f_last_name" />
                  <TextField fx:id="f_first_name" />
                  <TextField fx:id="f_username" />
                  <PasswordField fx:id="f_password" />
                  <PasswordField fx:id="f_password_confirm" />
                  <ComboBox fx:id="cb_department" maxWidth="1.7976931348623157E308" />
               </children>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </HBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="61.0" prefWidth="390.0" spacing="30.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="b_add" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="100.0" text="Ajouter" />
            <Button fx:id="b_cancel" mnemonicParsing="false" onAction="#quit" prefHeight="25.0" prefWidth="100.0" text="Annuler" />
         </children>
      </HBox>
   </bottom>
</BorderPane>s